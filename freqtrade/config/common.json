{
    "dry_run": true,
    "timeframe": "5m",
    "max_open_trades": 5,
    "unfilledtimeout": {
        "entry": 10,
        "exit": 10,
        "exit_timeout_count": 0,
        "unit": "minutes",
    },
    "entry_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1,
        },
    },
    "exit_pricing":{
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
    },
    "internals": {
        "process_throttle_secs": 30,
        "heartbeat_interval": 60,
    },
    "dataformat_ohlcv": "jsongz",
    "dataformat_trades": "jsongz",
    "freqai": {
        "enabled": true,
        "train_period_days": 200,
        "backtest_period_days": 0.125,
        "identifier": "unique-id",
        "live_retrain_hours": 0,
        "purge_old_models": false,
        "expiration_hours": 4,
        "continual_learning": true,
        "save_backtest_models": true,
        "keras": true,
        "model_save_type": "keras",
        "feature_parameters": {
            "include_timeframes": [
                "5m",
                //"15m",
                //"1h",
            ],
            "include_corr_pairlist": [
                "BTC/USDT",
                "ETH/USDT",
            ],
            "label_period_candles": 20,
            "include_shifted_candles": 2,
            "indicator_periods_candles": [
                100,
                200,
            ],
        },
        "model_training_parameters": {
            "n_estimators": 100,
        },
        "data_split_parameters": {
            "test_size": 0.33,
            "random_state": 1,
        },
    },
}
